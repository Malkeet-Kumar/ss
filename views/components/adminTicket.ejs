<style>
    .ticket{
        height: 150px;
        width: 300px;
        background-color: bisque;
        margin: 12px;
        padding: 8px;
        border-radius: 5px;
        box-shadow: 2px 2px 8px grey;
    }
    p{
        padding: 5px; 
        margin: 5px;
    }
    a{
        color: black;
        text-decoration: none;
    }
</style>
<a href="/ticket/<%= ticket.ticketId %>">
    <div class="ticket" id="<%= ticket.ticketId %>">
        <p >Title: <%= ticket.title %></p>
        <p>Raised By: <%= ticket.createdBy %></p>
        <p>Created on: <%= ticket.startTime %></p>
        <p>Expires on: <%= ticket.endTime %></p>
    </div>
</a>